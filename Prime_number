#include<iostream>
using namespace std;

int main()
{
    
    int n,count=0,i; 
    cin>>n;
    
    for(i=2; i<n; i++)
    {
      if(n%i==0)
        {
          count++;
          break;
        }
    }
    if(count==0)
    {
      cout<<"prime number"<<endl;
    }
    else
    {
      cout<<"not a prime number"<<endl;
    }
    
    
    return 0;
}
